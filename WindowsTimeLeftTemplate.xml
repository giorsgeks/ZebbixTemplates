                    <uuid>49855c144bfe443486fffec665c932d4</uuid>
                    <name>FDTimeleft2</name>
                    <type>CALCULATED</type>
                    <key>dtt</key>
                    <delay>360m</delay>
                    <units>s</units>
                    <params>timeleft(//vfs.fs.dependent.size[D:,pused],7d,100)</params>
                    <triggers>
                        <trigger>
                            <uuid>6ba122ebb5624cf1a01d619e01ccc31a</uuid>
